<div id="<%= dom_id activity %>">
  <p>
    <strong>Creator:</strong>
    <%= activity.creator %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= activity.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= activity.description %>
  </p>

  <p>
    <strong>Datetime:</strong>
    <%= activity.datetime %>
  </p>

  <p>
    <strong>Location:</strong>
    <%= activity.location %>
  </p>

  <p>
    <strong>Places:</strong>
    <%= activity.places %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= activity.price %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= activity.category %>
  </p>

</div>
